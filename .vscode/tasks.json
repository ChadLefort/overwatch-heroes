{
  "version": "0.1.0",
  "command": "npm",
  "isShellCommand": true,
  "args": [
    "run"
  ],
  "showOutput": "always",
  "tasks": [
    {
      "taskName": "build",
      "isBuildCommand": true
    },
    {
      "taskName": "clean"
    },
    {
      "taskName": "lint"
    },
    {
      "taskName": "test",
      "isTestCommand": true
    },
    {
      "taskName": "start"
    },
    {
      "taskName": "start:dev"
    },
    {
      "taskName": "deploy"
    },
    {
      "taskName": "watch",
      "isWatching": true
    },
    {
      "taskName": "watch:test",
      "isWatching": true
    }
  ]
}